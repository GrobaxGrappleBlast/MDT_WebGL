{
  "name": "mdt-webgl",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@salesforce/sfdx-lwc-jest": "^1.3.0",
    "@types/jest": "^29.4.0",
    "gltf-loader-ts": "^0.3.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-transform-stub": "^2.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-glsl": "^1.1.2",
    "webgl-strict-types": "^1.0.5"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "webgl": "^0.0.7"
  },
  "jestConfigurationSource": "// https://github.com/benawad/graphql-ts-server-boilerplate/blob/4_jest/package.json",
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^.+.(vert|frag|glsl)$": "jest-transform-stub"
    }
  }
}
